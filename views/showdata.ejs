<table
  id="example"
  class="table table-striped table-bordered"
  style="width: 100%;"
>
  <form action="/movies/findByUserID/?" method="GET">
    <input type="text" name="UserID" id="UserID" />
  </form>
  <thead>
    <tr>
      <th>No</th>
      <th>Movie Name</th>
      <th>Id Movie</th>
      <th>Genre</th>
      <th>Years</th>
      <th>Created By</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% for(let i= 0; i < results.length; i++) { %>
    <tr>
      <td><%= i+1; %></td>
      <td><%= results[i].name %></td>
      <td><%= results[i]._id %></td>
      <td><%= results[i].genre %></td>
      <td><%= results[i].year %></td>
      <!-- <td><%= results[i].UserID %></td> -->
      <td>
        <button class="btn btn-primary">Edit</button>
        <button class="btn btn-danger">Delete</button>
      </td>
    </tr>
    <% } %>
  </tbody>
</table>
